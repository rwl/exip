            </xs:annotation>
          </xs:element>
          <xs:element name="opModLVRT" type="CurvePairType" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Specify curveId pair for curve type == 5</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="opModStorConnect" type="xs:boolean" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Set storage DER as connected (true) or disconnected (false)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="opModVoltVAr" type="UInt16" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Specify curveId for curve type == 1</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="opModVoltWatt" type="UInt16" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Specify curveId for curve type == 4</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="opModWattPF" type="UInt16" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Specify curveId for curve type == 3</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DERControlList">
    <xs:annotation>
      <xs:documentation>A List element to hold DERControl objects.</xs:documentation>
