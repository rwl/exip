              <xs:documentation>Specifies the type of the curves defined in this curve group.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DERCurveListLink" type="DERCurveListLink" minOccurs="0" maxOccurs="1"/>
