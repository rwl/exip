  <xs:complexType name="IPInterface">
    <xs:annotation>
      <xs:documentation>Specific IPInterface resource.  This resource may be thought of as network status information for a specific network (IP) layer interface.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Resource">
        <xs:sequence>
          <xs:element name="ifDescr" type="String192" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifHighSpeed" type="UInt32" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifInBroadcastPkts" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifIndex" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifInDiscards" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Input Datagrams Discarded.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifInErrors" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifInMulticastPkts" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Multicast Datagrams Received.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifInOctets" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Bytes Received.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifInUcastPkts" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Datagrams Received.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifInUnknownProtos" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Datagrams with Unknown Protocol Received.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifMtu" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifName" type="String16" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  16 character size was verified in Linux (if.h)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifOperStatus" type="UInt8" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules and assignments from RFC 2863.  Max size is unclear to me.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifOutBroadcastPkts" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Broadcast Datagrams Sent.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifOutDiscards" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Output Datagrams Discarded.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifOutErrors" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifOutMulticastPkts" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Multicast Datagrams Sent.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifOutOctets" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Bytes Sent.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifOutUcastPkts" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.  Can be thought of as Datagrams Sent.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifPromiscuousMode" type="xs:boolean" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifSpeed" type="UInt32" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules from RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ifType" type="UInt16" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Use rules and assignments from RFC 2863.  Max size is unclear to me.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IPAddrListLink" type="IPAddrListLink" minOccurs="0" maxOccurs="1"/>
          <xs:element name="lastResetTime" type="Int64" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Similar to ifLastChange in RFC 2863.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="lastUpdatedTime" type="Int64" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>The date/time of the reported status.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LLInterfaceListLink" type="LLInterfaceListLink" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
