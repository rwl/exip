        <xs:sequence>
          <xs:element name="ActiveDERControlListLink" type="ActiveDERControlListLink" minOccurs="0" maxOccurs="1"/>
          <xs:element name="DERControlListLink" type="DERControlListLink" minOccurs="0" maxOccurs="1"/>
          <xs:element name="DERCurveGroupListLink" type="DERCurveGroupListLink" minOccurs="0" maxOccurs="1"/>
          <xs:element name="primacy" type="PrimacyType" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Indicates the relative primacy of the provider of this Program.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DERProgramList">
