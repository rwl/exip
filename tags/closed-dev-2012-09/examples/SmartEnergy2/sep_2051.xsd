  <xs:complexType name="LLInterface">
    <xs:annotation>
      <xs:documentation>A link-layer interface object.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Resource">
        <xs:sequence>
          <xs:element name="CRCerrors" type="UInt32" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Contains the number of CRC errors since reset.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EUI64" type="HexBinary64" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Contains the EUI-64 of the link layer interface. 48 bit MAC addresses SHALL be changed into an EUI-64 using the method defined in IETF RFC 4291, Appendix A. (Use "0xFFFE")</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IEEE_802_15_4" type="IEEE_802_15_4" minOccurs="0" maxOccurs="1"/>
          <xs:element name="linkLayerType" type="UInt8" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Specifies the type of link layer interface associated with the IPInterface. Values are below.
0 = Unspecified 
1 = IEEE 802.3 (Ethernet)
2 = IEEE 802.11 (WLAN)
3 = IEEE 802.15 (PAN)
4 = IEEE 1901 (PLC)
5-255 = Reserved</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LLAckNotRx" type="UInt32" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Number of times an ACK was not received for a frame transmitted (when ACK was requested).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LLCSMAFail" type="UInt32" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Number of times CSMA failed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LLFramesDropRx" type="UInt32" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Number of dropped receive frames.</xs:documentation>
            </xs:annotation>
          </xs:element>
