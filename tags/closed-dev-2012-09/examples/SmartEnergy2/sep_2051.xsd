              <xs:documentation>Contains the curve identifier assigned to this curve definition.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="rampTime" type="UInt16" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Ramp time for moving from current setpoint to new setpoint, in seconds.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="xMultiplier" type="PowerOfTenMultiplierType" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Multiplier for X-axis.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="xUnit" type="UomType" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>The X-axis units of measure.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="y1Multiplier" type="PowerOfTenMultiplierType" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Multiplier for Y1-axis</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="y1RefType" type="DERUnitRefType" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>The Y1-axis units context.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="y1Unit" type="UomType" minOccurs="1" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>The Y1-axis units of measure.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="y2Multiplier" type="PowerOfTenMultiplierType" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Multiplier for Y2-axis</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="y2RefType" type="DERUnitRefType" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>The Y2-axis units context.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="y2Unit" type="UomType" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>The Y2-axis units of measure.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DERCurveList">
    <xs:annotation>
      <xs:documentation>A List element to hold DERCurve objects.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="List">
        <xs:sequence>
          <xs:element name="DERCurve" type="DERCurve" minOccurs="0" maxOccurs="unbounded"/>
